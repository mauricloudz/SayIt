<p id="notice"><%= notice %></p>
<div class="text-center">
<p class="my-5">
  <strong>@<%= @tweet.user.username %> said:</strong><br>
  <i>"<%= @tweet.content %>"</i>
</p>

<small>
Liked by: <br>
<% @tweet.liking_users.each do |user| %>
<%= image_tag(user.avatar, class: 'avatar-in-tweet rounded') %>
<% end %>
</small>
</div>
<br>
<div class="text-center">
<%= link_to 'Back', root_path, class: 'btn btn-primary' %>
</div>
